#!/bin/bash

# source cockerenv
if [[ X$COCKER_HOME == X ]]
then
   x0=`which $0`
   x1=$(dirname $x0)
   if [[ ${x1} == "." ]]
   then
      x1=`pwd -P`
   fi
   x2=$(dirname $x1)

   COCKER_HOME=$x2
#  export COCKER_HOME
fi


CP1=$COCKER_HOME/java
CP1=${CP1}:$COCKER_HOME/lib/ivy.jar
CP1=${CP1}:$COCKER_HOME/lib/postgresql.jar
CP1=${CP1}:$COCKER_HOME/lib/asm9.jar

for f in $COCKER_HOME/lib/*
do
    fname=$(basename $f)
    if [[ ${fname} == commons-logging*.jar ]]
    then
	CP1=${CP1}:$f
    fi
done

for f in $COCKER_HOME/lib/*
do
    fname=$(basename $f)
    if [[ ${fname} == commons-el*.jar ]]
    then
	CP1=${CP1}:$f
    fi
done


for f in $COCKER_HOME/lib/*
do
    fname=$(basename $f)
    if [[ ${fname} == jtar*.jar ]]
    then
	CP1=${CP1}:$f
    fi
done

CP1=${CP1}:$COCKER_HOME/lib/commons-io-2.4.jar

for f in $COCKER_HOME/lib/*
do
    fname=$(basename $f)
    if [[ ${fname} == lucene*5.0.0.jar ]]
    then
	CP1=${CP1}:$f
    fi
done

for f in $COCKER_HOME/lib/eclipsejar/*
do
    fname=$(basename $f)
    if [[ ${fname} == *.jar ]]
    then
	CP1=${CP1}:$f
    fi
done

CP1=${CP1}:$COCKER_HOME/resources

java -cp ${CP1} edu.brown.cs.cocker.application.ApplicationServerRegulation $*
